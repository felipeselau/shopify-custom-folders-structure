        },    
        {
          "type": "text",
          "id": "margin_left_box",
          "label": "t:sections.slideshow.blocks.image.settings.margin_left_box.label"
        },   
        {
          "type": "text",
          "id": "margin_top_box",
          "label": "t:sections.slideshow.blocks.image.settings.margin_top_box.label"
        },   
